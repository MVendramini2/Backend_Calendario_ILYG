// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}


model Reserva {
  id      Int    @id @default(autoincrement())
  sala    String // "A" o "B"
  date    String // "YYYY-MM-DD"
  start   String // "HH:mm"
  end     String // "HH:mm"
  persona String
  area    String
  motivo  String

  createdAt DateTime @default(now())
}
